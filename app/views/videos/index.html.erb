<div class="grid grid24">
  <div class="row">
    <div class="col">
      <ul class="action-menu pull-right">
        <li>
          <%= link_to "New Video", new_video_path %>
        </li>
      </ul>
      <table class="table">
        <thead>
          <tr>
            <th>Name</th>
          </tr>
        </thead>
        <% @videos.each do |video| %>
          <tr>
            <td><%= link_to video.name, video %></td>
          <tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
